<h1>Listing music</h1>

<div >
<table class="table table-striped">
<% @products.each do |product| %>
  <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
    <td>
      <!-- <%= image_tag(product.image_url, class: 'list_image') %> -->
      <%= image_tag("http://www.umflint.edu/sites/default/files/groups/CSEP/images/unknown_user_0.jpg", class: 'list_image') %>
    </td>
    <td class="list_description">
      <dl>
        <dt><%= product.title %></dt>
        <!-- <dd><%= truncate(strip_tags(product.description), length: 80) %></dd> -->
        <dd><iframe frameborder="no" border="0" marginwidth="0" marginheight="0" width=298 height=52 src="<%= product.description %>"></iframe></dd>
      </dl>
    </td>
    <td class="list_actions">
      <%= link_to 'Show', product %><br/>
      <%= link_to 'Edit', edit_product_path(product) %><br/>
      <%= link_to 'Delete', product, method: :delete,
                  data: { confirm: 'Are you sure?' } %>
    </td>
  </tr>
<% end %>
</table>
<br />
<button class="btn btn-default"><span class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span><%= link_to 'New Music', new_product_path %></button>
</div>
      </request>

     <setdynvars sourcetype="random_number" start="0" end="99">
        <var name="dyn_search_id" />
      </setdynvars>

      <thinktime value="3" random="true"></thinktime>
      <request>
        <http url='/search' version='1.1' method='GET'></http>
